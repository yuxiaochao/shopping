<section class="flat-account background">
	<div class="container">
		<div class="row">
			<div class="col-md-6">
				<div class="form-login">
					<div class="title">
						<h3>登录</h3>
					</div>
					
						<div class="form-box">
							<label for="name-login">用户名 * </label> <input
								type="text" id="name-login" name="name-login" placeholder="Ali">
						</div>
						<!-- /.form-box -->
						<div class="form-box">
							<label for="password-login">密码 * </label> <input
								type="password" id="password-login" name="password-login"
								placeholder="******">
						</div>
						<!-- /.form-box -->
						<div class="form-box checkbox">
							<input type="checkbox" id="remember" checked name="remember">
							<label for="remember">记住密码</label>
						</div>
						<!-- /.form-box -->
						<div class="form-box">
							<button type="submit" id= "login" class="login">登录</button>
							<a title="">忘记密码</a>
						</div>
						<!-- /.form-box -->
					
					<!-- /#form-login -->
				</div>
				<!-- /.form-login -->
			</div>
			<!-- /.col-md-6 -->
			<div class="col-md-6">
				<div class="form-register">
					<div class="title">
						<h3>注册</h3>
					</div>
					
						<div class="form-box">
							<label for="name-register">用户名 * </label> <input
								type="text" id="name-register" name="name-register">
						</div>
						<!-- /.form-box -->
						<div class="form-box">
							<label for="password-register">密码</label> <input
								type="password" id="password-register" name="password-register">
						</div>
						<div class="form-box">
							<label for="phone-register">手机号* </label> <input
								type="text" id="phone-register" name="phone-register">
						</div>
						<!-- /.form-box -->
						<div class="form-box" style="position: absolute;top: 432px;">
							<button type="submit" id="register" class="register">注册</button>
						</div>
						<!-- /.form-box -->
					
					<!-- /#form-register -->
				</div>
				<!-- /.form-register -->
			</div>
			<!-- /.col-md-6 -->
		</div>
		<!-- /.row -->
	</div>
	<!-- /.container -->
</section>
